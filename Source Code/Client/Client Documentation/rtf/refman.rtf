NSURLConnection for mocking.\par
Use with {\b GHAsyncTestCase} to mock out connections. {
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid      @end\par
\par
@implementation GHNSURLConnectionMockTest\par
\par
- (void)testMock \{\par
  [self prepare];\par
  GHMockNSURLConnection *connection = [[GHMockNSURLConnection alloc] initWithRequest:nil delegate:self];   \par
  [connection receiveHTTPResponseWithStatusCode:204 headers:testHeaders_ afterDelay:0.1];\par
  [connection receiveData:testData_ afterDelay:0.2];\par
  [connection finishAfterDelay:0.3];\par
  [self waitForStatus:kGHUnitWaitStatusSuccess timeout:1.0];\par
\}\par
\par
- (void)connection:(NSURLConnection *)connection didReceiveResponse:(NSURLResponse *)response \{\par
  GHAssertEquals([(NSHTTPURLResponse *)response statusCode], 204, nil);\par
  GHAssertEqualObjects([(NSHTTPURLResponse *)response allHeaderFields], testHeaders_, nil);\par
\}\par
\par
- (void)connection:(NSURLConnection *)connection didReceiveData:(NSData *)data \{\par
  GHAssertEqualObjects(data, testData_, nil);\par
\}\par
\par
- (void)connectionDidFinishLoading:(NSURLConnection *)connection \{\par
  [self notify:kGHUnitWaitStatusSuccess forSelector:@selector(testMock)];\par
\}\par
@end}
 \par
}}
