<% provide(:title, 'All Application Users') %>
<div class="block">
<div class="grid">
              
    <div class="grid-title">
     <div class="pull-left">
        <div class="icon-title"><i class="icon-user"></i></div>
        <span>App Users</span> 
        <div class="clearfix"></div>
     </div>
    <div class="clearfix"></div>   
    </div>
  
    <div class="grid-content overflow">
      
      <table class="table table-bordered" id="datatable_2">
      <thead>
        <tr class="t_center">
            <th>First Name</th>
            <th>Last Name</th>
            <th class="hidden-mobile">User Type</th>
            <th></th>
          </tr>
      </thead>
       <tbody>
            <% (@appuser).each do |appuser| %>
            <tr>
                <td> <%= appuser.firstName %> </td>
                <td> <%= appuser.lastName %> </td>   
                <td> <%= type_of_appuser(appuser.userType) %> </td> 
                <td><%= link_to "View", appuser %></td>  
            </tr>
            <% end %> 
        </tbody>
    </table>

                
              <div class="clearfix"></div>
              </div>
              
              </div>
            </div>



